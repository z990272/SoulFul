@foreach (var m in (List<Soulful.ViewModels.HitViewModel>)ViewData["TotalHits"])
{
    <div class="single_new_item d-flex align-items-center justify-content-between">
        <div class="d-flex align-items-center">
            <div class="thumbnail">
                <img src="@m.Pic" alt="">
            </div>
            <div class="content_">
                <h6>@m.Name</h6>
                <p>@m.Singer</p>
            </div>
        </div>
        <div class="audioplayer" id="@m.Id" onclick="videoShow(this)">
            <div class="audioplayer_playpause"><a href="#" type="button"></a></div>
        </div>
    </div>
}


